if __name__ == '__main__':
    n = int(input().strip())
    if n%2:
        print ("Weird")
    else:
        if n>2 and n<5:
            print ("Not Weird")
        elif n>6 and n<22:
            print ("Weird")
        else:
            print("Not Weird")
